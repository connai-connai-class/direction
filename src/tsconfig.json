{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "isolatedModules": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./resources/js/React/*"],
      "@pages/*": ["./resources/js/React/Pages/*"],
      "@layouts": ["./resources/js/React/Layouts/index.tsx"],
      "@ui-elements": ["./resources/js/React/Components/ui-elements/index.tsx"],
      "@ui-parts": ["./resources/js/React/Components/ui-parts/index.tsx"],
      "@icons": ["./resources/js/React/Components/ui-elements/Icons/index.tsx"],
      "@columns": ["./resources/js/React/Components/Column/index.js"]
    }
  },
  "include": [
    "resources/js/**/*.ts",
    "resources/js/**/*.tsx",
    "resources/js/**/*.d.ts"
  ]
}
